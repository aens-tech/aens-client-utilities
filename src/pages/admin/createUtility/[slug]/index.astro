---
export const prerender = false

import BaseInterection from "@/layouts/BaseInterection.layout.astro";

import AdminDashboardComponent from "@/components/admin/adminDashboard.component";
import CreateUtilityComponent from "@/components/admin/createUtility/createUtility.component";

const { slug } = Astro.params;
---

<BaseInterection
    title={"AENS - Convocatoria"}
    description={"Estas apunto de ser parte de una convocatoria de AENS TECH."}
	showDecorations={false}
    disableCenteredOnVertical={true}
>
	<CreateUtilityComponent
		slug={slug}
		client:load
	/>
</BaseInterection>
